<style>
  #suggestions {
      position: absolute;
      z-index: 1000; /* Ensure it appears above other elements */
    
  }
</style>
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
            <h4 class="card-title card_title_cus">Total vote of Employee</h4>
            <div class="table-responsive pt-3 custome_table_res">
              <table class="table_cus table-striped" style="width:100%" id="example">
                <thead>
                  <tr>
                    <th class="society_th_1">SL No</th>
                    <th class="society_th_6">Name</th>
                    <th class="society_th_9">Email</th>
                    <th class="society_th_9">Mobile NO.</th>
                    <th class="society_th_9">Total Vote</th>
                  </tr>
                </thead>
                <tbody id="item-list">
                  <% var i=1
                  data.forEach(dt => { %>
                      <tr>
                          <td><%= i++ %></td>
                          <td><%= dt.user_name %></td>
                          <td><%= dt.user_email %></td>
                          <td><%= dt.user_mobile %></td>
                          <td><%= dt.total_vote %></td>
                      </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
</div>